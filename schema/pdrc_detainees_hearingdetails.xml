
<schema adapter="tagabukid_pdrc">
  <element tablename="pdrc_detainees_hearingdetails">

	<field name="objid" primary="true" prefix="DHD" />
	<field name="detaineeid" required="true" searchable="true" visible="false" />
	<field name="courtbranch_objid" required="true" searchable="true" />
	<field name="hearingdate" required="false" searchable="true" />
	<field name="hearingtime" required="false" searchable="true" />
		
	<complex name="courtbranch" ref="pdrc_courts" jointype="many-to-one" required="false">
		<key field="courtbranch_objid" target="objid" /> 
	</complex>

  </element>
</schema>